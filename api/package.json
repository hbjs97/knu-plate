    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "nocache": "^2.1.0",
    "sequelize": "^6.3.5",
    "swagger-jsdoc": "^5.0.1",
    "swagger-ui-express": "^4.1.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
